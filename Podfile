source 'https://github.com/CocoaPods/Specs.git'

target :JotForm_iOS do
       use_frameworks!
       platform :ios, '9.0'
       pod 'Alamofire', '~> 4.5'
end

target :JotForm_Mac do
    use_frameworks!
    platform :osx, '10.10'
    pod 'Alamofire', '~> 4.5'
end

target :JotForm_WatchOS do
    use_frameworks!
    platform :watchos, '2.0'
    pod 'Alamofire', '~> 4.5'
end
