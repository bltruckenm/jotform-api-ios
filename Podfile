source 'https://github.com/CocoaPods/Specs.git'

target :JotForm_iOS do
       platform :ios, '8.0'
       pod 'AFNetworking/NSURLSession', '~> 3.0'
end

target :JotForm_Mac do
    platform :osx, '10.9'
    pod 'AFNetworking/NSURLSession', '~> 3.0'
end

target :JotForm_WatchOS do
    platform :watchos, '2.0'
    pod 'AFNetworking/NSURLSession', '~> 3.0'
end
